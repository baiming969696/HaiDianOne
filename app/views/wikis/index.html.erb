<h1>Wiki List</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Created Time</th>
  </tr>
 
  <% @wikis.each do |wiki| %>
    <tr>
      <td><%= wiki.name %></td>
      <td><%= wiki.description %></td>
      <td><%= wiki.created_at.localtime %></td>
      <td><%= wiki.updated_at.localtime %></td>
      <td><%= link_to 'Show', wiki_path(wiki) %></td>
      <td><%= link_to 'Edit', edit_wiki_path(wiki) %></td>
    </tr>
  <% end %>

</table>

<%= link_to 'Start a new wiki', new_wiki_path %>
